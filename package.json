{
  "name": "cracklibjs",
  "description": "WIP Pure JS Cracklib-like lib for Node",
  "version": "0.5.1",
  "author": {
    "name": "Zac Anger",
    "email": "zac@zacanger.com",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib",
    "cli.js"
  ],
  "bin": "./cli.js",
  "scripts": {
    "build": "echo 'no build yet'",
    "cover": "nyc npm run test:tape",
    "prebuild": "run-s -s test:lint cover",
    "preversion": "npm run build",
    "start": "node index",
    "test": "run-s -s test:lint test:tape",
    "test:lint": "eslint -c .eslintrc.json lib/",
    "test:tape": "tape lib/*.test.js"
  },
  "homepage": "https://github.com/zacanger/cracklibjs#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/cracklibjs.git"
  },
  "bugs": "https://github.com/zacanger/cracklibjs/issues",
  "keywords": [
    "cracklib",
    "libcrack",
    "password",
    "security"
  ],
  "dependencies": {
    "zeelib": "8.2.1",
    "zrgs": "1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "eslint": "5.5.0",
    "eslint-config-zacanger": "3.4.1",
    "eslint-plugin-babel": "5.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-unicorn": "6.0.1",
    "npm-run-all": "4.1.3",
    "nyc": "13.0.1",
    "tape": "4.9.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "nyc": {
    "include": [
      "lib/index.js"
    ],
    "sourceMap": true,
    "instrument": true,
    "reporter": [
      "text",
      "html",
      "json"
    ]
  },
  "os": [
    "freebsd",
    "linux",
    "openbsd"
  ]
}
